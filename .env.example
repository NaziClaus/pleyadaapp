# PostgreSQL configuration
DB_NAME=pleyada
DB_USER=pleyada
DB_PASS=secret

# SFTP server configuration
SFTP_HOST=example.com
SFTP_PORT=22
SFTP_USER=user
SFTP_PASS=pass
SFTP_DIR=/remote/path

# Local storage configuration
# Internal container directory for downloads
LOCAL_DIR=/data
# CSV log file path inside the container
CSV_PATH=/data/download_log.csv
# Path on the host machine where downloaded files will be stored
HOST_STORAGE=./downloads
